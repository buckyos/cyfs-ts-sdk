    "commander": "^8.1.0",
    "fs-extra": "^9.1.0",
    "google-protobuf": "^3.20.1",
    "hex-to-array-buffer": "^1.1.0",
    "inquirer": "^8.2.4",
    "inquirer-command-prompt": "^0.1.0",
    "js-sha256": "^0.9.0",
    "jsbi": "^3.2.1",
    "jsencrypt": "^3.0.0-rc.1",
    "minimist": "^1.2.6",
    "node-fetch": "^2.6.1",
    "node-localstorage": "^2.2.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "qrcode-terminal": "^0.12.0",
    "table": "^6.8.0",
    "ts-results": "^3.3.0",
    "tslib": "^2.1.0",
    "websocket": "^1.0.33"
  }
}
