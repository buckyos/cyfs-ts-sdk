    "build:tool": "set npm_config_node=true && webpack --config webpack.tool.config.js",
    "build:h5": "set npm_config_h5=true && webpack --config webpack.config.js",
    "watch": "tsc -w",
    "compile": "tsc"
  },
  "bin": {
